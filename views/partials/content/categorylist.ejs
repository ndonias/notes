<article class="categorylist">
    <% if (front.length > 0) { %>
    <h1>Cards</h1>
      <% for (i=1; i<front.length; i++) { %>
       
        <div id="card-<%=[i]%>" class="card"> 
		  <div class="front"> 
		    <%= front[i] %>
		  </div> 
		  <div class="back">
		    <%= back[i] %>
		  </div> 
		</div>

      <% } %>
    <% } %>

</article>
<script>
$(function($) {
	for (i=1; i<=10; i++){
  	$("#card-"+i).flip();
  };
});
</script>
